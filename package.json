{
  "name": "breaktout-game",
  "version": "0.0.1",
  "description": "Breakout game base on HTML5",
  "private": true,
  "scripts": {
    "pack": "npx webpack --mode=development",
    "build": "babel src -d lib && npx webpack",
    "test": "nyc --reporter=html --reporter=text mocha $(find ./src -name '*.spec.js') --recursive",
    "testing": "nyc --reporter=html --reporter=text mocha $(find ./src -name '*.spec.js') --recursive -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sshplendid/breakout-in-javascript.git"
  },
  "keywords": [
    "javascript",
    "vanillaJS",
    "breakout",
    "game",
    "HTML5"
  ],
  "author": "SeHun Shin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sshplendid/breakout-in-javascript/issues"
  },
  "homepage": "https://github.com/sshplendid/breakout-in-javascript#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "webpack": "^4.17.3",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  }
}
